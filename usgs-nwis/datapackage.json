{
  "name": "usgs-nwis",
  "title": "USGS NWIS Site Data",
  "description": "Discharge observations from nearby USGS NWIS sites.",
  "version": "0.1.0",
  "contributors": [
    {
      "name": "Ethan Welty",
      "email": "ethan.welty@gmail.com",
      "roles": "Assembled and published the datapackage."
    }
  ],
  "sources": [
    {
      "name": "USGS National Water Information System (NWIS)",
      "web": "https://nwis.waterdata.usgs.gov/nwis/"
    }
  ],
  "resources": [
    {
      "name": "knik-river",
      "title": "15281000 Knik River, AK",
      "path": "data/knik-river.csv",
      "schema": {
        "fields": [
          {
            "name": "datetime",
            "type": "date",
            "description": "Date over which the values are summarized (unknown whether UTC or local time)",
            "format": "YYYY-MM-DD"
          },
          {
            "name": "discharge",
            "type": "number",
            "description": "River discharge (ft^3 / s)."
          }
        ]
      }
    }
  ]
}
