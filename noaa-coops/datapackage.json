      "path": "data/air_temperature-9454240.csv",
      "schema": {
        "fields": [
          {
            "name": "t",
            "type": "datetime",
            "description": "Date and time (UTC).",
            "format": "YYYY-MM-DD'T'hh:mm:ss'Z'"
          },
          {
            "name": "v",
            "type": "number",
            "description": "Air temperature (째C)."
          }
